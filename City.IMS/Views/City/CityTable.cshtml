

@model City.IMS.ViewModels.CityListViewModel

<table class="table">
    <thead>
        <tr>
            <th scope="col">SL#</th>
            <th scope="col">Name</th>
            <th scope="col">No. of Dwellers</th>
            <th scope="col">Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var city in Model.citymodels)
        {
        <tr>
            <th scope="row">@city.Id</th>
            <td>@city.Name</td>
            <td>@city.Dwellers</td>
            <td>@city.Country.Name</td>

        </tr>
        }

    </tbody>
</table>