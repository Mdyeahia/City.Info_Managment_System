
@model City.IMS.ViewModels.CountryViewModel

<table class="table">
    <thead>
        <tr>
            <th scope="col">SL#</th>
            <th scope="col">Name</th>
            <th scope="col">About</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var country in Model.countries)
        {
            <tr>
                <th scope="row">@country.Id</th>
                <td>@country.Name</td>
                <td>@country.About</td>

            </tr>
        }

    </tbody>
</table>